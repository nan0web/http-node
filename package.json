{
	"name": "@nan0web/http-node",
	"version": "1.0.0",
	"description": "Node.js HTTP client and server built on native modules with minimal dependencies.",
	"main": "./src/index.js",
	"types": "./types/index.d.ts",
	"type": "module",
	"files": [
		"src/**/*.js",
		"!src/**/*.test.js",
		"types/**/*.d.ts"
	],
	"exports": {
		".": {
			"import": "./src/index.js",
			"types": "./types/index.d.ts"
		},
		"./client": {
			"import": "./src/client/index.js",
			"types": "./types/client/index.d.ts"
		},
		"./messages": {
			"import": "./src/messages/index.js",
			"types": "./types/messages/index.d.ts"
		},
		"./middlewares": {
			"import": "./src/middlewares/index.js",
			"types": "./types/middlewares/index.d.ts"
		},
		"./server": {
			"import": "./src/server/index.js",
			"types": "./types/server/index.d.ts"
		},
		"./test": {
			"import": "./src/test/index.js",
			"types": "./types/test/index.d.ts"
		}
	},
	"scripts": {
		"build": "tsc",
		"test": "node --test \"src/**/*.test.js\"",
		"test:coverage": "node --experimental-test-coverage --test-coverage-include=\"src/**/*.js\" --test-coverage-exclude=\"src/**/*.test.js\" --test \"src/**/*.test.js\"",
		"test:coverage:collect": "nan0test coverage",
		"test:docs": "node --test src/README.md.js",
		"test:release": "node --test \"releases/**/*.test.js\"",
		"test:status": "nan0test status --hide-name --debug",
		"play": "node play/main.js",
		"precommit": "npm test",
		"prepush": "npm test",
		"prepare": "husky",
		"release": "nan0release publish",
		"clean": "rm -rf .cache/ && rm -rf dist/",
		"clean:modules": "rm -rf node_modules"
	},
	"keywords": [
		"nano",
		"nanoweb",
		"nanoweb-http-node",
		"http"
	],
	"author": "ЯRаСлав YaRaSlove <support@yaro.page>",
	"license": "ISC",
	"devDependencies": {
		"@nan0web/http": "^1.0.0",
		"@nan0web/test": "^1.0.2",
		"husky": "^9.1.7"
	},
	"peerDependencies": {
		"@nan0web/http": "^1.0.0"
	}
}
